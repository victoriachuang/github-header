{
	"manifest_version": 2,
	"name": "Remove dark Github header",
	"version": "1.0",
	"description": "Remove dark Github header",
	"content_scripts":
	[
		{
			"matches": ["*://github.com/*", "*://gist.github.com/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
	"icons": {
		"16": "./favicon/favicon-16.png",
		"48": "./favicon/favicon-48.png",
		"128": "./favicon/favicon-128.png"
	}
}
